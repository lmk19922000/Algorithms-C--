#include "stdio.h"
#include "iostream"
#include <queue>
#include <functional>

using namespace std;

int main(){
	long long num;
	int count;
	priority_queue<int, std::vector<int>, std::greater<int> > heap;

	heap.push(1);
	while (count < 1500){
		num = heap.pop();

	}

	printf("The 1500'th ugly number is %d.", num);
}